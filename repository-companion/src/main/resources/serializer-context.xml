    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedTicket"
        class="org.aldica.repo.ignite.binary.value.TicketBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedDuration"
        class="org.aldica.repo.ignite.binary.value.DurationBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedAuthenticationCacheEntry"
        class="org.aldica.repo.ignite.binary.misc.AuthenticationCacheEntryBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedExecutionDetails"
