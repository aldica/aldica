<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="Configuration.${moduleId}.core.binary.serializer.configurableRelevantFields"
        class="${aldica.ignite-common.basePackage}.binary.SelectivelyReflectiveBinarySerializer">
        <property name="relevantFieldsProvider">
            <bean class="${aldica.ignite-common.basePackage}.binary.ConfigurableSerializationRelevantFieldsProvider">
                <property name="basePropertyKey" value="${moduleId}.core.binary.relevantFields" />
                <property name="propertiesSource" ref="global-properties" />
            </bean>
        </property>
    </bean>

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedTxnCacheRegionKey"
        class="org.aldica.repo.ignite.binary.support.TransactionalCacheRegionKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedTxnCacheValueHolder"
        class="org.aldica.repo.ignite.binary.support.TransactionalCacheValueHolderBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedCacheRegionKey"
        class="org.aldica.repo.ignite.binary.support.CacheRegionKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedCacheRegionValueKey"
        class="org.aldica.repo.ignite.binary.support.CacheRegionValueKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedNodeVersionKey"
        class="org.aldica.repo.ignite.binary.support.NodeVersionKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedChildByNameKey"
        class="org.aldica.repo.ignite.binary.support.ChildByNameKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedPucKey"
        class="org.aldica.repo.ignite.binary.support.CachePucKeyBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedTransactionEntity"
        class="org.aldica.repo.ignite.binary.entity.TransactionEntityBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedStoreEntity"
        class="org.aldica.repo.ignite.binary.entity.StoreEntityBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedNodeEntity"
        class="org.aldica.repo.ignite.binary.entity.NodeEntityBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedChildAssocEntity"
        class="org.aldica.repo.ignite.binary.entity.ChildAssocEntityBinarySerializer" />

    <bean id="Configuration.${moduleId}.core.binary.serializer.alfresco.optimisedAclEntity"
